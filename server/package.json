{
  "name": "boulevardtcg-market-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "dev:db": "prisma generate && prisma migrate deploy",
    "docker:up": "docker compose -f ../docker-compose.yml up -d",
    "docker:down": "docker compose -f ../docker-compose.yml down",
    "build": "tsc",
    "start": "node dist/main.js",
    "pretest": "npm run test:setup",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "vitest",
    "test:setup": "node -e \"require('fs').mkdirSync('.db',{recursive:true})\" && cross-env NODE_ENV=test DATABASE_URL=\"file:./.db/test.db\" prisma generate --schema prisma/test/schema.prisma && cross-env NODE_ENV=test DATABASE_URL=\"file:./.db/test.db\" prisma migrate deploy --schema prisma/test/schema.prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "job:tcgdex": "tsx src/jobs/tcgdexDailySnapshot.ts",
    "job:cardmarket-json": "tsx src/jobs/importCardmarketJson.ts",
    "job:boutique-prices": "tsx src/jobs/importPricesFromBoutique.ts",
    "tcgdex:snapshot:all": "tsx scripts/tcgdex-snapshot-all.ts",
    "tcgdex:snapshot:all:fr-en-ja": "cross-env TCGDEX_LANGS=fr,en,ja tsx scripts/tcgdex-snapshot-all.ts"
  },
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@prisma/client": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.0",
    "cross-env": "^7.0.3",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
